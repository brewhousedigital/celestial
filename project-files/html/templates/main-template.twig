<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="Content-Security-Policy" content="default-src https:">
    <link rel="manifest" href="/manifest.json">
    <link rel="icon" sizes="192x192" type="image/png" href="/images/icons/icon-192x192.png">
    <meta name="theme-color" content="{{ site.metaThemeColor }}">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <link rel="apple-touch-icon" href="/images/icons/icon-72x72.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/images/icons/icon-152x152.png">
    <link rel="apple-touch-icon" sizes="192x192" href="/images/icons/icon-192x192.png">

    {% set versionControl = "1" %}

    <title>{{ page.title is not null ? page.title ~ " | " ~ site.title : site.title }}</title>
    <meta name="description" content="{{ page.description is not null ? page.description : site.description }}">


    <meta property="og:url"                content="{{ site.fullURL }}" />
    <meta property="og:title"              content="{{ site.title }}" />
    <meta property="og:description"        content="{{ site.description }}" />
    <meta property="og:image"              content="{{ site.openGraphImage }}" />
    <meta property="og:image:width"        content="971" />
    <meta property="og:image:height"       content="509" />
    <meta property="og:type"               content="{{ page.OpenGraphType is not null ? page.OpenGraphType : 'website' }}" />


    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@200;600&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/library-prism.css">
    <link rel="stylesheet" href="/css/styles.css?v={{ versionControl }}">


    {# Loads the tracking analytics code in your header #}
    {% include('templates/optional/analytics.twig') %}

    {# Loads the recaptcha library with the publishable key #}
    {% include('templates/optional/recaptcha.twig') %}


</head>
<body>

{% include('templates/header.twig') %}


<div id="main">
    {# Load the requested page. If that page does not exist, give the user a 404 error. #}
    {% include [page.template ~ '.twig', '404.twig'] %}
</div><!-- end main -->


{% include('templates/footer.twig') %}


{# Put your libraries here #}

<script src="/js/framework-setup.js?v={{ versionControl }}"></script>
<script src="/js/api-functions.js?v={{ versionControl }}"></script>
<script src="/js/framework-core.js?v={{ versionControl }}"></script>
<script src="/js/scripts.js?v={{ versionControl }}"></script>


</body>
</html>